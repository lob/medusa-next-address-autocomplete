  },
  "devDependencies": {
    "bench": "^0.3.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.12.0",
    "eslint-config-brightspace": "^0.2.1",
    "istanbul": "^0.4.3",
