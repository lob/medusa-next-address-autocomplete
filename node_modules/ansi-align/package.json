  },
  "devDependencies": {
    "ava": "^2.0.0",
    "chalk": "^2.4.2",
    "coveralls": "^3.0.3",
    "nyc": "^14.0.0",
    "standard": "^14.0.0",
    "standard-version": "^7.0.0"
  }
}